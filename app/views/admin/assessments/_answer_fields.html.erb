<div class="panel panel-default nested-fields answer-container">
	<div class="panel-body">
		<%= f.label :Answer %></br>
		<%= f.text_field :title, class: 'form-control' %></br>
		<%= link_to_remove_association raw('<i class="fa fa-trash-o"></i>Remove Answer'), f, class: 'btn btn-default btn-sm', data: {toggle: "tooltip", placement: "left" } %>
	</div>
</div>
